{
  "$schema": "https://raw.githubusercontent.com/koriym/spec/master/schemas/alps.json",
  "alps": {
    "doc": {
      "value": "[Example] ALPS Profile"
    },
    "version": "0.1",
    "descriptor": [
      {
        "id": "Index",
        "type": "semantic",
        "doc": {
          "value": "Index Page"
        }
      },
      {
        "id": "About",
        "type": "semantic",
        "def": "https://www.iana.org/assignments/link-relations/link-relations.xhtml"
      },
      {
        "id": "Blog",
        "type": "semantic",
        "def": "https://schema.org/Blog",
        "doc": {
          "value": "Blog entry list page"
        },
        "descriptor": [
          {
            "href": "#BlogPosting"
          },
          {
            "id": "blogPosting",
            "type": "safe",
            "rt": "#BlogPosting"
          },
          {
            "id": "item",
            "type": "safe",
            "def": "https://www.iana.org/assignments/link-relations/link-relations.xml",
            "rt": "#BlogPosting"
          },
          {
            "id": "post",
            "type": "unsafe",
            "def": "http://activitystrea.ms/specs/json/1.0/#post-verb",
            "descriptor": [
              {"href": "#BlogPosting"}
            ]
          },
          {
            "id": "about",
            "type": "safe",
            "rt": "#About"
          }
        ]
      },
      {
        "id": "BlogPosting",
        "type": "semantic",
        "def": "https://schema.org/BlogPosting",
        "doc": {
          "value": "Blog entry item page"
        },
        "descriptor": [
          {
            "id": "articleBody",
            "type": "semantic",
            "def": "https://schema.org/articleBody"
          },
          {
            "id": "dateCreated",
            "type": "semantic",
            "def": "https://schema.org/BlogPosting#dateCreated"
          },
          {
            "id": "collection",
            "type": "safe",
            "def": "https://www.iana.org/assignments/link-relations/link-relations.xhtml",
            "rt": "#Blog"
          }
        ]
      }
    ]
  }
}
